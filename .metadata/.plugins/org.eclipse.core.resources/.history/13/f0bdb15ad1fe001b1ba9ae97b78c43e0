package tuts20;

class Frog {
	private String name;
	private int id;
	
	public Frog(int id, String name) {
		this.id = id;
		this.name = name;
	}
	public String toString() {
		return id +_": " + name;
	}
}
public class Appies {
	public static void main(String[] args) {
		Frog frog1 = new Frog(7, "bobby");
	
		System.out.println(frog1);
	}
}
